ANTHROPIC_RETRY_ATTEMPTS=3

# Google Gemini
GEMINI_API_URL=https://generativelanguage.googleapis.com/v1
GEMINI_API_KEY=your-gemini-api-key
GEMINI_DEFAULT_MODEL=gemini-1.5-pro
GEMINI_TIMEOUT=30
GEMINI_RETRY_ATTEMPTS=3

